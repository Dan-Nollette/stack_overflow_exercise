<h1>Stack Overflow</h1>
<% if @questions.any? %>
  <ul>
    <% @questions.each do |question| %>
      <li>
        <%= link_to question.vote, question_path(question) %>
        <%= link_to question.responses.length, question_path(question) %>
        <%= link_to question.title, question_path(question) %>
      </li>
    <% end %>
  </ul>
<% else %>
  <p>Oops! Currently we dont have question....</p>
<% end %>
<button><%= link_to 'Add Question', new_question_path() %></button>
